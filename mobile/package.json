{
  "name": "motohub",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,md}\" --write",
    "format:check": "prettier \"**/*.{js,jsx,ts,tsx,json,md}\" --check",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run format:check && npm run typecheck && npx expo-doctor",
    "prebuild:sha": "node write-git-sha.js",
    "build:android:release": "npm run prebuild:sha && expo run:android --variant release",
    "build:ios:release": "npm run prebuild:sha && expo run:ios --configuration release"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
